import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int A[][] = new int[x][10];
        int n = 3;
        
        for(int i=0;i<x;i++) {for(int l=0;l<10;l++) {A[i][l] = sc.nextInt();}}
        for(int i=0;i<x;i++) {Arrays.sort(A[i]);}
        for(int i=0;i<x;i++) {System.out.println(A[i][10-3]);}
        
        sc.close();
    }
}
